{
    "name": "fheerdink/datatypes",
    "description": "Several data types that can be used for your projects",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Fheerdink\\DataTypes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fheerdink\\DataTypes\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Franke Heerdink",
            "email": "frankeheerdink@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5",
        "phpstan/phpstan": "1.5",
        "squizlabs/php_codesniffer": "3.6.2",
        "slevomat/coding-standard": "7.1"
    }
}
